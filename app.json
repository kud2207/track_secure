{
  "expo": {
    "name": "trackSecure 1.0.1",
    "slug": "trackSecure",
    "version": "1.4.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trackSecure",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icone.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.trackSecure"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icone.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBUCAoEiQ45e-Vhcv3mKOZpfWtAZ2YCZPQ"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.trackSecure"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow isMyOwn to use your location"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow trackSecure to access your camera",
          "microphonePermission": "Allow trackSecure to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
